; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Node-16 Framework"
config/features=PackedStringArray("4.0", "GL Compatibility")
boot_splash/image="res://Branding/Icons/Universal/icon.png"
boot_splash/fullsize=false
config/icon="res://Branding/Icons/Universal/icon.svg"

[display]

window/size/viewport_width=320
window/size/viewport_height=224
window/size/window_width_override=960
window/size/window_height_override=672
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/BitmapTileCollision/plugin.cfg", "res://addons/Node-16 Framework/plugin.cfg")

[rendering]

renderer/rendering_method.mobile="gl_compatibility"
